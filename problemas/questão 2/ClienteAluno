public class ClienteAluno {
    public static void main(String[] args) {
        double[] notas = { 8.5, 7.0, 9.2 };
        Aluno aluno = new Aluno("João", 12345, notas);

        System.out.println("Informações do Aluno:");
        System.out.println("Nome: " + aluno.getNome());
        System.out.println("Identificação: " + aluno.getIdentificacao());
        System.out.println("Notas: " + Arrays.toString(aluno.getNotas()));
        System.out.println("Média: " + aluno.calcularMedia());
        System.out.println("Aprovado: " + aluno.estaAprovado());
    }
}
